{
  "name": "scarab-carapace",
  "version": "4.0.0-beta.10",
  "description": "Highly configurable framework for generating functional CSS classes",
  "main": "core.scss",
  "scripts": {
    "prebuild": "rm -f test/build/*",
    "build": "node-sass --include-path node_modules test/src/test.scss -o test/build/",
    "postbuild": "npm run split && npm run minify && npm run gzip",
    "split": "sakugawa -n 9999999999 -m separate test/build/test.css",
    "preminify": "rm -f test/build/*.min.css",
    "minify": "for file in test/build/*.css\ndo\n cssnano \"$file\" \"$file\".min \ndone",
    "gzip": "for file in test/build/*.css.min\ndo\n tar -zcvf \"$file\".tar.gz \"$file\" \ndone",
    "test": "node-sass --include-path node_modules test/src/test.scss -o test/build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/watchtowerdigital/scarab-carapace.git"
  },
  "keywords": [
    "sass",
    "scss",
    "css",
    "atomic",
    "functional",
    "framework",
    "responsive",
    "front-end"
  ],
  "author": "Kyle Oliveiro <kyle@watchtower.digital>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/watchtowerdigital/scarab-carapace/issues"
  },
  "homepage": "https://github.com/watchtowerdigital/scarab-carapace#readme",
  "dependencies": {
    "scarab-scss": "^7.0.0-beta.7"
  },
  "devDependencies": {
    "cssnano-cli": "^1.0.5",
    "node-sass": "^4.7.2",
    "sakugawa": "^0.5.3"
  }
}
