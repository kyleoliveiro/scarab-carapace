  },
  "homepage": "https://github.com/watchtowerdigital/carapace#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "browser-sync": "^2.12.10",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.6.2",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
